<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.colorful.mapper.userMapper">
    <select id="userVerify" resultType="com.colorful.pojo.User">
        select * from user_data where name = #{name} and password = #{password};

    </select>

    <select id="selectAll" resultType="com.colorful.pojo.User">
        select * from user_data;
    </select>

</mapper>

